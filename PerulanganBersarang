package whilebilanganganjil;

import java.util.Scanner;
public class WhileBilanganGanjil {

    /**
     * @hurinrazanah
     */
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        System.out.println("While bilangan ganjil");
        System.out.print("Masukkan nilai n: ");
        int n = s.nextInt();
        int i = 1; 

        while (i <= n) {
            if (i % 2 != 0) {
                System.out.println(i);
            }
            i++;
        }
    }
}
